---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <link>{{ site.url }}{{ site.baseurl | default: "" }}/</link>
    <description>{{ site.description | xml_escape }}</description>
    <language>en-us</language>
    <atom:link href="{{ site.url }}{{ site.baseurl | default: "" }}/feed.xml" rel="self" type="application/rss+xml"/>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <link>{{ site.url }}{{ site.baseurl | default: "" }}{{ post.url }}</link>
      <description>{{ post.excerpt | xml_escape }}</description>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <guid isPermaLink="true">{{ site.url }}{{ site.baseurl | default: "" }}{{ post.url }}</guid>
      {% for tag in post.tags %}
      <category>{{ tag | xml_escape }}</category>
      {% endfor %}
    </item>
    {% endfor %}
  </channel>
</rss>
